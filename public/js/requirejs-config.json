{
    "baseUrl": "/public/js/",
    "buildUrl": "/public/build/",
    "preserveLicenseComments": false,
    "generateSourceMaps": true,
    "optimize": "uglify2",
    "paths": {
        "jquery": "../vendor/jquery-2.0.0",
        "jquery.hotkeys": "../vendor/jquery.hotkeys",
        "jquery.validate": "../vendor/jquery.validate.min",
        "jquery.autosize": "../vendor/jquery.autosize.min",
        "jquery-ui": "../vendor/jquery-ui-1.10.3.custom.min",
        "underscore": "../vendor/underscore",
        "backbone": "../vendor/backbone",
        "backbone.marionette": "../vendor/backbone.marionette",
        "bootstrap": "../vendor/bootstrap-3.2.0-dist/js/bootstrap.min",
        "bootstrap-datetimepicker": "../vendor/bootstrap-datetimepicker/bootstrap-datetimepicker.min",
        "moment": "../vendor/moment.min",
        "moment-timezone": "../vendor/moment-timezone-with-data-2010-2020.min",
        "jstz": "../vendor/jstz",
        "sockjs": "../vendor/sockjs-0.3"
    },
    "shim": {
        "jquery": { "exports": "jQuery" },
        "jquery-ui": { "deps": ["jquery"], "exports": "jQuery.fn.ui" },
        "jquery.hotkeys": { "deps": ["jquery"], "exports": "jQuery.hotkeys" },
        "jquery.validate": { "deps": ["jquery"], "exports": "jQuery.fn.validate" },
        "jquery.autosize": { "deps": ["jquery"], "exports": "jQuery.fn.autosize" },
        "underscore": { "exports": "_" },
        "backbone": { "deps": ["jquery", "underscore"], "exports": "Backbone" },
        "backbone.marionette": { "deps": ["backbone"], "exports": "Backbone.Marionette" },
        "bootstrap": { "deps": ["jquery"], "exports": "jQuery.fn.tooltip" },
        "bootstrap-datetimepicker": { "deps": ["bootstrap"], "exports": "jQuery.fn.datetimepicker" },
        "jstz": { "exports": "jstz" },
        "sockjs": { "exports": "sockjs" }
    },
    "modules": {
        "index.js": { "include": "index" },
        "events.js": { "include": "events" },
        "event-app.js": { "include": "event-app" },
        "event-static.js": { "include": "event-static" },
        "admin.js": { "include": "admin" },
        "admin-users.js": { "include": "admin-users" },
        "admin-event.js": { "include": "admin-event" },
        "myevents.js": { "include": "myevents" },
        "permalink.js": { "include": "permalink" },
        "permalink-intro.js": { "include": "permalink-intro" },
        "post_event_followup_email_preview.js": { "include": "post_event_followup_email_preview" },
        "facilitator.js": { "include": "facilitator" }
    }
}